&ext_memory{
	status = "disabled";
};
&clk_lsi {
	status = "okay";
};

&clk_hsi48 {
	status = "disabled";
};

&clk_hse {
	clock-frequency = <DT_FREQ_M(24)>;
	status = "disabled";
};

&clk_lse {
	status = "disabled";
};

&pll {
	div-m = <12>;
	mul-n = <200>;
	div-p = <2>;
	div-q = <2>;
	div-r = <2>;
	div-s = <2>;
	div-t = <2>;
	clocks = <&clk_hse>;
	status = "disabled";
};

&rcc {
	clocks = <&clk_lsi>;
	clock-frequency = <DT_FREQ_M(64)>;
	dcpre = <1>;
	hpre = <1>;
	ppre1 = <1>;
	ppre2 = <1>;
	ppre4 = <1>;
	ppre5 = <1>;
};

